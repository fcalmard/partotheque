{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Tessitures</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Codetessiture</th>
                <td>{{ entity.CodeTessiture }}</td>
            </tr>
            <tr>
                <th>Libelletessiture</th>
                <td>{{ entity.libelleTessiture }}</td>
            </tr>
            <tr>
                <th>Datecreateat</th>
                <td>{{ entity.datecreateAt|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Actif</th>
                <td>{{ entity.actif }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('tessitures') }}">
            Back to the list
        </a>
    </li>
</ul>
{% endblock %}
